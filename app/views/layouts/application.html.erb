<!DOCTYPE html>
<html>
  <head>
    <title>Massage My Way: LA's Premier Mobile Massage</title>
    <%= stylesheet_link_tag 'screen' %>
    <%= javascript_include_tag :defaults, "jquery.tools.js" %>
    <%= csrf_meta_tag %>


<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-23500847-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

  </head>
  <body>
    <div id="body">
      <div id="page">
        <div id="header">
          <a href="/" id="logo">
          <h1>Massage My Way</h1>
          </a>
          <div class="wrapper">
            <span class="left">
              <strong>Massage My Way</strong> - LA's Premier Mobile Massage
            </span>
            <span class="right">
              <a href="/bookings/new" rel="modal"><strong>Book Online</strong></a> or
              <strong>Call: (310) 853-1830</strong>
            </span>
          </div>
          <div id="nav" class="clearfix">
            <%= render_navigation %>
          </div>
        </div>
        
        <div class="main">
          <div id="content">

            <div class="box_top_left">
              <div class="box_top_right">
                <div class="box_top">
                </div>
              </div>
            </div>
            <div class="box_left">
              <div class="box_right">
                <div class="box">
                  <div class="pad">
                    <%= yield %>
                  </div>
                </div>
              </div>
              <div class="box_bot_left">
                <div class="box_bot_right">
                  <div class="box_bot">
                  </div>
                </div>
              </div>
          </section>
        </div>
        <div id="footer" class="clearfix">
          <span class="left">
            &copy; 2010
            <strong>Massage My Way</strong> &nbsp;|&nbsp;
            <%= link_to "Our Policies", policies_path %>
          </span>
        </div>
      </div>
    </div>
  </body>
</html>